algoritmo "Contador"
var
   contador, valor, salto: inteiro
inicio
      contador <- 0
      Escreva("Informe até que valor você quer contar: ")
      Leia(valor)
      Escreva("Informe o valor do salto: ")
      Leia(salto)
      Enquanto (contador <= valor) faca
               Escreval(contador)
               contador <- contador + salto
      FimEnquanto



fimalgoritmo